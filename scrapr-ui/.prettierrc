{
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "jsxBracketSameLine": true,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 100,
  "endOfLine": "auto",
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowSpacing": true,
  "embeddedLanguageFormatting": "auto"
}
